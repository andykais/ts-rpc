{
  "name": "sample_v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:localdeps": "npm pack ../build/ && mkdir -p deps && mv ./ts-rpc-1.0.0.tgz deps",
    "preinstall": "echo pwd: $PWD && npm run build:localdeps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "rm -fr build && npm run build:client && npm run build:server",
    "build:client": "echo pwd: $PWD && ls -l node_modules && node_modules/webpack-cli && webpack --config webpack.config.browser.js",
    "build:server": "tsc --outDir build/server",
    "start": "node build/server/server/index.js",
    "compile:client:watch": "webpack --watch --config webpack.config.browser.js"
  },
  "keywords": [],
  "author": "Andrew Kaiser",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "ts-rpc": "file:deps/ts-rpc-1.0.0.tgz",


    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "ts-loader": "^8.0.1",
    "typescript": "^4.0.0-beta",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
  }
}
